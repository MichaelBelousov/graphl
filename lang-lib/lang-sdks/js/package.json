{
  "name": "@graphl/compiler-js",
  "exports": {
    "./native-backend": {
      "types": "./dist/types/index.d.mts",
      "default": "./dist/native-cjs/index.js"
    },
    "./wasm-backend": {
      "types": "dist/types/index.d.mts",
      "default": "./dist/esm/index.js"
    },
    ".": {
      "require": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.js"
      }
    },
    "./raw": {
      "default": "./index.mts"
    }
  },
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "browser": "// TODO UMD",
  "types": "dist/types/index.d.mts",
  "//type": "// FIXME: make this whichever you want?",
  "private": true,
  "scripts": {
    "build": "rollup -c && bun run build-types && bun run build-native",
    "rebuild": "rm -r dist && bun run build",
    "build-debug": "NODE_ENV=development rollup -c && bun run build-types",
    "build-types": "tsc -p ./tsconfig.json",
    "build-native": "node ./native-build.mjs",
    "dev": "rollup -c -w",
    "test": "bun test ./test/e2e.test.ts",
    "test-node": "node --loader=node-zigar --experimental-transform-types ./test/e2e.test.ts",
    "test-node-only": "node --loader=node-zigar --test-only --experimental-transform-types ./test/e2e.test.ts",
    "test-debug": "node --inspect-brk --loader=node-zigar --test-only --experimental-transform-types ./test/e2e.test.ts",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-zigar": "^0.14.0",
    "esbuild": "^0.25.3",
    "rollup": "^4.40.0",
    "rollup-plugin-define": "^1.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-zigar": "^0.14.0",
    "typescript": "^5",
    "vite": "^6.3.2",
    "vite-plugin-inspect": "^11.0.1"
  },
  "dependencies": {
    "node-zigar": "^0.14.0"
  },
  "files": [
    "dist"
  ]
}
